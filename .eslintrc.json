{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    // This will we can still have unused variables in the code, just add an underscore before the variable name
    "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }],
    "no-console": "warn"
  }
}
